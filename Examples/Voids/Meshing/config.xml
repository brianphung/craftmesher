 </refinement>

 <coarsening>
   <coarsening_passes>8</coarsening_passes>
   <SurfaceTargetEdgeLength>3.0</SurfaceTargetEdgeLength>
   <VolumeTargetEdgeLength>3.0</VolumeTargetEdgeLength>
   <EdgeSmoothing>2</EdgeSmoothing>
